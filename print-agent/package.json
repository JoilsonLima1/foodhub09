{
  "name": "foodhub-print-agent",
  "version": "2.0.0",
  "description": "FoodHub Print Agent — impressão térmica ESC/POS via spooler do Windows",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "pkg": "pkg dist/index.js --targets node20-win-x64 --output dist/FoodHubPrintAgent.exe"
  },
  "dependencies": {
    "express": "4.21.0",
    "node-thermal-printer": "4.4.2",
    "printer": "0.4.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.14.0",
    "tsx": "4.16.0",
    "typescript": "5.4.5",
    "pkg": "5.8.1"
  }
}
