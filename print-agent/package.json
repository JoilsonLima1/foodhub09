{
  "name": "foodhub-print-agent",
  "version": "1.0.0",
  "description": "FoodHub Print Agent — impressão térmica 1-clique sem diálogo do navegador",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "pkg": "pkg dist/index.js --targets node18-win-x64 --output dist/FoodHubPrintAgent.exe"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "pdf-to-printer": "^5.6.0",
    "puppeteer": "^23.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.0",
    "pkg": "^5.8.1"
  }
}
